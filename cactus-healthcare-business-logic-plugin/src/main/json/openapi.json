{
  "openapi": "3.0.3",
  "info": {
    "title": "Hyperledger Cactus Example - Health Care EHR App",
    "description": "Demonstrates EHR management across multiple distinct Hyperledger Fabric ledgers.",
    "version": "v2.0.0-alpha.2",
    "license": {
      "name": "Apache-2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "components": {
    "schemas": {
      "HospitalNetworkA": {
        "type": "object",
        "required": ["id", "HIdB"],
        "properties": {
          "id": {
            "type": "string",
            "nullable": false,
            "minLength": 1,
            "maxLength": 1024
          },
          "HIdB": {
            "description": "The foreign key ID referencing the corresponding entry in HospitalNetworkB.",
            "type": "string",
            "minLength": 1,
            "maxLength": 1024,
            "nullable": false
          }
        }
      },
      "HospitalNetworkB": {
        "type": "object",
        "required": ["id"],
        "properties": {
          "id": {
            "type": "string",
            "nullable": false,
            "minLength": 1,
            "maxLength": 1024
          }
        }
      },
      "InsertDataRequest": {
        "type": "object",
        "required": ["data"],
        "properties": {
          "data": {
            "$ref": "#/components/schemas/HospitalNetworkA"
          }
        }
      },
      "InsertDataResponseHSPA": {
        "type": "object",
        "properties": {
          "callOutput": {
            "type": "object",
            "additionalProperties": true
          },
          "transactionReceipt": {
            "type": "object",
            "additionalProperties": true
          }
        }
      },
      "ListHSPAResponse": {
        "type": "object",
        "required": ["data"],
        "properties": {
          "data": {
            "type": "array",
            "nullable": false,
            "default": [],
            "items": {
              "$ref": "#/components/schemas/HospitalNetworkA"
            },
            "minItems": 0,
            "maxItems": 65535
          }
        }
      },
      "ListHSPBResponse": {
        "type": "object",
        "required": ["data"],
        "properties": {
          "data": {
            "type": "array",
            "nullable": false,
            "default": [],
            "items": {
              "$ref": "#/components/schemas/HospitalNetworkB"
            },
            "minItems": 0,
            "maxItems": 65535
          }
        }
      }
    }
  },
  "paths": {
    "/api/v1/plugins/@hyperledger/cactus-healthcare-backend/insert-patient-hspa": {
      "post": {
        "x-hyperledger-cactus": {
          "http": {
            "verbLowerCase": "post",
            "path": "/api/v1/plugins/@hyperledger/cactus-healthcare-backend/insert-patient-hspa"
          }
        },
        "operationId": "InsertDataRequestV1",
        "summary": "Inserts the provided data entity to the ledger.",
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InsertDataRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "CREATED",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InsertDataResponseHSPA"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/plugins/@hyperledger/cactus-healthcare-backend/list-patient-hspa": {
      "get": {
        "x-hyperledger-cactus": {
          "http": {
            "verbLowerCase": "get",
            "path": "/api/v1/plugins/@hyperledger/cactus-healthcare-backend/list-patient-hspa"
          }
        },
        "operationId": "listDataV1",
        "summary": "Lists all the Data entities stored on the ledger.",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ListHSPAResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/plugins/@hyperledger/cactus-healthcare-backend/list-patient-hspb": {
        "get": {
          "operationId": "ListHSPBResponseV1",
          "x-hyperledger-cacti": {
            "http": {
              "verbLowerCase": "get",
              "path": "/api/v1/plugins/@hyperledger/cactus-healthcare-backend/list-patient-hspb"
            }
          },
          "summary": "Lists all the BambooHarvest entities stored on the ledger.",
          "parameters": [],
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/ListHSPBResponse"
                  }
                }
              }
            }
          }
        }
      }
  }
}
